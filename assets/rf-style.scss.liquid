//Christmas

#christmas {
  background-color: $red;
  height: 100vh;

  .site-header {
    background-color: transparent !important;
  }
  .breadcrumb-wrapper {
    display: none !important;
  }
  .rf-wrapper {
    background-color: $red;
    height: 84vh;

    @include media-query($medium-up){
      height: 88vh;
    }
  }
  .rf-christmas-wrapper {
    position: relative;
  }
  .christmas-frame-wrapper {
    height: 54vh;

    @include media-query($medium-up){
      height: 68vh;
    }

    .christmas-frame__inner__background {
      background-color: #fee7da;
      position: absolute;
      bottom: 10px;
      top: 10px;
      left: 50%;
      width: 100%;
      z-index: 0;
      transform: translateX(-50%);

      @include media-query($medium-up){
        width: 33vw;
      }

      @include media-query($widescreen){
        width: 26vw;
      }

      .christmas__copy {
        padding: 10px;
        padding-left: 20px;

        h1, h2, p {
          color: #333;
        }

        h1  {
          font-size: 45px;
          margin-bottom: 8px;
        }
        h2 {
          color: $red;
          font-size: 45px;
          margin-bottom: 12px;
        }

        p {
          color: #333;
          font-size: 22px;
        }
      }
    }

    .christmas-frame__inner {
      position: absolute;
      bottom: 10px;
      top: 10px;
      left: 50%;
      overflow: hidden;
      width: 100%;
      transform: translateX(-50%);
      transition: all ease-in-out 0.5s;
      z-index: 10;
      cursor: pointer;
      background-color: darken($red,  20%);

      @include media-query($medium-up) {
        width: 33vw;
      }

      @include media-query($widescreen) {
        width: 26vw;
      }

      &.open {
        left: -48%;

        @include media-query($medium-up) {
          left: 16%;
        }
      }

      h1 {
        color: #fff;
        font-size: 30px;
        margin-left: 10px;
        margin-top: 10px;
        margin-bottom: 0;

        @include media-query($widescreen) {
          font-size: 50px;
        }
      }

      p {
        color: lighten($red, 20%);
        font-size: 22px;
        margin-left: 10px;
        margin-top: 10px;
      }

      img {
        height: 58vw;
        width: 43vw;
        object-fit: cover;
        position: absolute;
        bottom: 0;
        left: 50%;
        transform: translateX(-50%);

        @include media-query($medium-up) {
          height: 20vw;
          width: 15vw;
        }
      }
    }
  }
  .christmas-frame-text {
    position: relative;
    margin-top: 10px;
    width: 100%;
    text-align: center;

    @include media-query($medium-up) {
      position: absolute;
      bottom: 20px;
      margin-top: 0;
    }

    h1 {
      font-size: 30px;
      line-height: 0;
      margin-bottom: 6px;
      color: #fff;

      @include media-query($medium-up) {
        font-size: 45px;
      }
    }
    h2 {
      color: #fff;
      margin-bottom: 5px;
      font-size: 18px;

      @include media-query($medium-up) {
        font-size: 23px;
      }
    }
  }

  .rf-button {
    border: 1px solid #fff;
    padding: 10px;
    border-radius: 25px;
    width: max-content;
    margin-left: auto;
    margin-right: auto;
    color: #fff;

    a {
      color: #121212;
    }

    &.left {
      margin-right: unset;
      margin-left: unset;
      border: 1px solid #333;
      color: #333;
    }

    &:hover {
      background-color: $yellow;
      border: $yellow 1px solid;
      color: #333;
    }
  }
  #shopify-section-footer {
    display: none;
  }
}

//YUK

@keyframes wobble {
  0% {
    transform: rotate(0deg);
  }
  25% {
    transform: rotate(3deg);
  }
  50% {
    transform: rotate(-3deg);
  }
  100% {
    transform: rotate(0deg);
  }
}

$beige: #f1eedf;
$redyuk: #f97372;

.wobble {
  animation-name: wobble;
  animation-duration: 4s;
  animation-timing-function: linear;
  animation-iteration-count: infinite;
  transform-origin: bottom;
}

@keyframes spin {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(-360deg);
  }
}

